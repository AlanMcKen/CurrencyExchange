@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<table>
    <tr class="header">
        <td><p>Название книги</p></td>
        <td><p>Автор</p></td>
        <td><p>Цена</p></td>
        <td></td>
    </tr>
    @foreach (CurrencyExchange.Models.Office b in Model)
    {
        <tr>
            <td><p>@b.name</p></td>
            <td><p>@b.city</p></td>
            <td><p>@b.adress</p></td>
            <td><p><a href="/Home/Buy/@b.Id">Купить</a></p></td>
        </tr>
    }
</table>



